<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
xmlns="http://xmlns.jcp.org/xml/ns/javaee" 
xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd" 
version="3.1">
  <display-name>BookingAttraction</display-name>
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
    <welcome-file>index.htm</welcome-file>
    <welcome-file>index.jsp</welcome-file>
    <welcome-file>default.html</welcome-file>
    <welcome-file>default.htm</welcome-file>
    <welcome-file>default.jsp</welcome-file>
  </welcome-file-list>
  

  <servlet-mapping>
 <servlet-name>UserServlet</servlet-name>
<url-pattern>/UserServlet/dashboard</url-pattern>
 <url-pattern>/UserServlet/edit</url-pattern>
 <url-pattern>/UserServlet/update</url-pattern>
 <url-pattern>/UserServlet/delete</url-pattern>
 </servlet-mapping>

  <servlet>
 <servlet-name>AttractionServlet</servlet-name>
 <servlet-class>AttractionServlet</servlet-class>
 </servlet>

 <servlet-mapping>
 <servlet-name>AttractionServlet</servlet-name>
<url-pattern>/AttractionServlet/dashboard</url-pattern>
 <url-pattern>/AttractionServlet/edit</url-pattern>
 <url-pattern>/AttractionServlet/update</url-pattern>
 <url-pattern>/AttractionServlet/delete</url-pattern>
 </servlet-mapping>
 
   <servlet>
 <servlet-name>GetBookingServlet</servlet-name>
 <servlet-class>GetBookingServlet</servlet-class>
 </servlet>

 <servlet-mapping>
 <servlet-name>GetBookingServlet</servlet-name>
<url-pattern>/GetBookingServlet/dashboard</url-pattern>
 <url-pattern>/GetBookingServlet/edit</url-pattern>
 <url-pattern>/GetBookingServlet/update</url-pattern>
 <url-pattern>/GetBookingServlet/delete</url-pattern>
 </servlet-mapping>
 
 
 
</web-app>